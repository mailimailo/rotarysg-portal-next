{
  "name": "rotary-portal",
  "version": "1.0.0",
  "description": "Rotary Club St.Gallen - Lunch und Speaker Management Portal",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "import-lunches": "node import-lunches-production.js"
  },
  "keywords": [
    "rotary",
    "management",
    "lunch",
    "speaker"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
